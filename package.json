  "main": "index.js",
  "scripts": {
    "start": "apidoc -i ./src/controller  -o ./api-doc/doc && node ./src/server.js",
    "dev": "apidoc -i ./src/controller  -o ./api-doc/doc && nodemon ./src/server.js",
    "docs": "apidoc -i ./src/controller  -o ./api-doc/doc",
    "local": "nodemon --exec babel-node ./src/server.js",
    "prod": " npm run docs && node ./src/server.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npm run clean && npm run build:docs && babel ./src -d build/src",
    "clean": "rm -rf build && mkdir build",
    "build:docs": "apidoc -i ./src/controller  -o ./build/api-doc/doc",
    "db:migrate": "npx sequelize-cli db:migrate",
    "seed-all": "npx sequelize-cli db:seed:all"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.733.0",
